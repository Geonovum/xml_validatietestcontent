                        <Hoofdstuk eId="chp_2" wId="gm0297_1__chp_2">
                            <Kop>
                                <Label>Hoofdstuk</Label>
                                <Nummer>2</Nummer>
                                <Opschrift>Functietoedeling</Opschrift>
                            </Kop>
